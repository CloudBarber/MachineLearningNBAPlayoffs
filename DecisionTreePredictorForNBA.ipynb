    "def gini(rows):\n",
    "    # TODO: your code...\n",
    "    values = class_counts(rows)\n",
    "    impurity = 1\n",
    "    for value in values:\n",
    "        probability = values[value] / float(len(rows))\n",
    "        impurity -= probability**2\n",
    "    return impurity"
